{
  "compilerOptions": {
    "noImplicitAny": false,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "outDir": "build/server",
    "pretty": true,
    "sourceMap": true,
    "lib": ["es2020"],
    "module": "commonjs",
    "target": "es2020",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "*.ts"
  ]
}


